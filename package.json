{
  "name": "whatsapp-food-catalog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A React application for food catalogs with WhatsApp cart integration",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "deploy": "wrangler deploy",
    "check": "tsc -b && wrangler deploy --dry-run",
    "lint": "eslint .",
    "preview": "wrangler deploy --dry-run --outdir=dist && wrangler dev",
    "cf-typegen": "wrangler types && node --eval \"require('fs').rmSync('worker-configuration.d.ts')\""
  },
  "dependencies": {
    "framer-motion": "^12.0.0",
    "hono": "^4.8.2",
    "lucide-react": "^0.468.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.7.5",
    "@cloudflare/workers-types": "^4.20241127.0",
    "@eslint/js": "^9.27.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.6",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.0",
    "wrangler": "^4.21.0"
  }
}
